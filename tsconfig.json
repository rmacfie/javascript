{
    "include": [
        "src/**/*"
    ],
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": false,
        "suppressImplicitAnyIndexErrors": true,
        "target": "es6",
        "moduleResolution": "node",
        "removeComments": true,
        "sourceMap": true,
        "noLib": false,
        "declaration": true,
        "outDir": "dist"
    }
}
